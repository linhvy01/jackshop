<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
		

		<move element="brandsLogo" destination="page.wrapper" before="footer-container"/>
		<move element="homeLogoDeskTop" destination="header-wrapper" after="logo"/>
		<move element="welcomeDr" destination="page.wrapper" after="brandsLogo"/>

		<container name="brandsLogo" htmlTag="section" htmlClass="ppl-brands">
		   <block class="Magento\Cms\Block\Block" name="brandLogo">
		        <arguments>
		            <argument name="block_id" xsi:type="string">ppl-brands</argument>
		        </arguments>
		    </block>
		</container>

		<container name="welcomeDr" htmlTag="section" htmlClass="welcome-dr">
		   <block class="Magento\Cms\Block\Block" name="welcomeDrEarth">
		        <arguments>
		            <argument name="block_id" xsi:type="string">welcome-dr</argument>
		        </arguments>
		    </block>
		</container>

		<container name="homeLogoDeskTop" htmlTag="div" htmlClass="home-logo-desktop">
		   <block class="Magento\Cms\Block\Block" name="desktopLogoMain">
		        <arguments>
		            <argument name="block_id" xsi:type="string">desktop-logo</argument>
		        </arguments>
		    </block>		
		</container>

    <body/>
</page>
